{
  "name": "moria-backend",
  "version": "1.0.0",
  "description": "Backend Node.js + SQLite3 para Moria Peças & Serviços",
  "main": "server.js",
  "scripts": {
    "dev": "nodemon server.js",
    "start": "node server.js",
    "migrate": "knex migrate:latest",
    "migrate:rollback": "knex migrate:rollback",
    "seed": "knex seed:run",
    "db:reset": "knex migrate:rollback:all && knex migrate:latest && knex seed:run"
  },
  "keywords": [
    "nodejs",
    "express",
    "sqlite3",
    "knex",
    "api"
  ],
  "author": "Moria Peças & Serviços",
  "license": "MIT",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-rate-limit": "^8.1.0",
    "express-slow-down": "^3.0.0",
    "helmet": "^8.0.0",
    "ioredis": "^5.7.0",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.2",
    "rate-limit-redis": "^4.2.2",
    "sharp": "^0.34.3",
    "sqlite3": "^5.1.7",
    "uuid": "^13.0.0",
    "winston": "^3.17.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@types/node": "^22.16.5",
    "nodemon": "^3.1.9"
  }
}
